{
	"manifest_version": 2,

	"name": "iPhone to Ugly Duckling",
    "short_name": "Ugly Duckling",
	"description": "Replaces the text 'iPhone' with 'Ugly Duckling'.",
	"author": "Elizabeth Ferrao",
	"version": "1.1",
	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},

	"content_scripts":
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}

